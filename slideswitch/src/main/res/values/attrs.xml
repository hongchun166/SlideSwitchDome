<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SlideSwitch">
        <attr name="mSS_close_color" format="color"/>
        <attr name="mSS_open_color" format="color"/>
        <attr name="mSS_thunm_color" format="color"/>
        <attr name="mSS_isThumLine" format="boolean"/>
        <attr name="mSS_isThumShapeEquilateral" format="boolean"/>
        <attr name="mSS_isOpen" format="boolean"/>
        <attr name="mSS_textOpen" format="string"/>
        <attr name="mSS_textClose" format="string"/>
        <attr name="mSS_textSize" format="dimension"/>
        <attr name="mSS_shape" >
            <enum name="rect" value="1"/>
            <enum name="circle" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="AlphabetView">
        <attr name="mAV_bgColor" format="color"/>
        <attr name="mAV_bgColorSelect" format="color"/>
        <attr name="mAV_textColor" format="color"/>
        <attr name="mAV_textColorSelect" format="color"/>
        <attr name="mAV_textSize" format="dimension"/>
        <attr name="mAV_textSizeSelect" format="dimension"/>
        <attr name="mAV_popBgColor" format="color"/>
        <attr name="mAV_popTextColor" format="color"/>
        <attr name="mAV_popTextSize" format="dimension"/>
        <attr name="mAV_popW" format="dimension"/>
        <attr name="mAV_popH" format="dimension"/>
    </declare-styleable>

</resources>